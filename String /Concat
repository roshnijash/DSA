// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

int main() {
    // Write C++ code here
    
    string s="hello";
    string t="worldsss";
    string v =s;
// concatenation

cout<<s<<" "<<t<<"\n";

// or with append fn
string st=s.append(t);
cout<<st<<"\n";

//Length 
int lengths=v.size();
cout<<lengths<<"\n";



